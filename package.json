{
  "name": "angular-boilerplate",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "./env.sh dev && npm start -- -c development",
    "build": "./env.sh prod && ng build --output-path=./dist/out",
    "build:dev": "./env.sh dev && ng build -c development",
    "analyze": "ng build -c production --stats-json && webpack-bundle-analyzer ./dist/angular-boilerplate/stats.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch false",
    "test:watch": "ng test",
    "eslint": "node_modules/.bin/eslint --config .eslintrc.yml",
    "lint": "npm run eslint -- src --ext .ts,.html",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install",
    "prettier": "node_modules/.bin/prettier src --config .prettierrc.yml",
    "prettier:check": "npm run prettier -- --check",
    "prettier:fix": "npm run prettier -- --write"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.2",
    "@angular/cdk": "^12.2.11",
    "@angular/common": "~12.1.2",
    "@angular/compiler": "~12.1.2",
    "@angular/core": "~12.1.2",
    "@angular/forms": "~12.1.2",
    "@angular/material": "^12.2.11",
    "@angular/platform-browser": "~12.1.2",
    "@angular/platform-browser-dynamic": "~12.1.2",
    "@angular/router": "~12.1.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.2",
    "@angular-eslint/builder": "12.5.0",
    "@angular-eslint/eslint-plugin": "12.5.0",
    "@angular-eslint/eslint-plugin-template": "12.5.0",
    "@angular-eslint/schematics": "12.5.0",
    "@angular-eslint/template-parser": "12.5.0",
    "@angular/cli": "~12.1.2",
    "@angular/compiler-cli": "~12.1.2",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "angular-html-parser": "^1.8.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jasmine": "^4.1.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "^2.4.1",
    "typescript": "~4.3.2",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
